                    <article class="slds-tile slds-media slds-card__tile slds-hint-parent">
                        <div class="slds-media__body">
                            <div class="slds-tile__detail">
                                <div class="slds-form-element">
                                    <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_left">
                                        <svg class="slds-icon slds-input__icon slds-input__icon_left slds-icon-text-default" aria-hidden="true">
                                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="~lib/salesforce-lightning-design-system/assets/icons/utility-sprite/svg/symbols.svg#search" />
                                        </svg>
                                        <input type="text" id="tweets-filter--input" class="slds-input" placeholder="Filter tweets containing." />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </article>
                </li>
            </ul>
        </div>
        <footer class="slds-card__footer"></footer>
    </article>
</div>
